{
  "name": "xlsx-database-migration",
  "version": "1.0.0",
  "description": "Node.js TypeScript CLI tool for migrating Google Sheets XLSX files to PostgreSQL database",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:fast": "tsc --transpileOnly",
    "start": "node dist/index.js",
    "start:ts": "ts-node --transpile-only src/index.ts",
    "dev": "nodemon --exec \"ts-node --transpile-only src/index.ts\"",
    "test": "node test-migration.js",
    "test:quick": "npm run start:ts -- --config-only --verbose",
    "validate": "npm run start:ts -- --dry-run --verbose"
  },
  "keywords": [
    "xlsx",
    "google-sheets",
    "postgresql",
    "migration",
    "typescript",
    "cli"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@types/xlsx": "^0.0.35",
    "dotenv": "^16.6.1",
    "google-auth-library": "^9.14.0",
    "googleapis": "^140.0.1",
    "pg": "^8.12.0",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "@types/pg": "^8.11.8",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
